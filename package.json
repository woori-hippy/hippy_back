{
  "name": "express-box",
  "version": "0.0.2",
  "description": "Make API-Endpoints for Ethereum BlockChain",
  "scripts": {
    "start": "cross-env NODE_ENV=dev babel-node ./src/index.js",
    "dev": "cross-env NODE_ENV=dev nodemon",
    "build": "babel src -d dist",
    "prisma:build": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:update": "npx prisma db push --preview-feature && px prisma generate",
    "truffle:test": "truffle test test/*",
    "truffle:build": "truffle compile --all && truffle migrate --reset && truffle migrate --network development"
  },
  "author": "Arvind & Pranav",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/runtime-corejs3": "^7.13.10",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^24.9.0",
    "jest-environment-node": "^24.9.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.0.4",
    "prisma": "^2.20.1",
    "truffle-contract": "^1.1.11",
    "web3": "^0.20.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@pinata/sdk": "^1.1.13",
    "@prisma/client": "^2.20.1",
    "babel-jest": "^24.9.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "luxon": "^1.26.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "supertest": "^4.0.2"
  }
}
